{
    "name": "attendize/attendize",
    "version": "2.6.0",
    "description": "A free and open-source event management and ticket selling application.",
    "keywords": [
        "event management",
        "ticket selling",
        "tickets",
        "events"
    ],
    "license": "AAL",
    "type": "project",
    "homepage": "https://www.attendize.com",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "doctrine/dbal": "^2.13",
        "dompdf/dompdf": "^2.0",
        "fideloper/proxy": "^4.4",
        "graham-campbell/markdown": "^13.1",
        "guzzlehttp/guzzle": "^6.4",
        "guzzlehttp/psr7": "^2.4",
        "intervention/image": "^2.7",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^7.29",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/omnipay": "^3.2",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^1.7",
        "mews/purifier": "^3.3",
        "milon/barcode": "^9.0",
        "nitmedia/wkhtml2pdf": "dev-master",
        "omnipay/common": "^3.2",
        "omnipay/dummy": "^3.0",
        "omnipay/paypal": "^3.0",
        "omnipay/stripe": "^3.2",
        "php-http/curl-client": "^2.2",
        "php-http/message": "^1.13",
        "predis/predis": "^2.0",
        "stripe/stripe-php": "^9.6",
        "superbalist/php-money": "^2.0",
        "vinelab/http": "~1.5",
        "fruitcake/laravel-cors": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpspec/phpspec": "^7.2",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0",
        "fakerphp/faker": "^1.9.1"
    },
    "autoload": {
        "classmap": [
            "app/Http/Controllers",
            "app/Models",
            "app/Attendize",
            "app/Services",
            "app/Services/PaymentGateway",
            "database",
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Attendize\\": "app/Models"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php -r \"copy('.env.example', '.env');\"",
            "@php artisan key:generate"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php -r \"copy('.env.example', '.env');\"",
            "@php artisan key:generate"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "potsky/laravel-localization-helpers"
            ]
        }
    },
    "minimum-stability": "stable"
}
